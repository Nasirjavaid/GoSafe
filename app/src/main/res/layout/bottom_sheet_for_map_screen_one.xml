                            android:text=" -"
                            android:layout_marginTop="@dimen/margin_top_bottom_sheet_Items"
                            android:textSize="@dimen/bottom_sheet_text_size_items_below"
                            android:textColor="@color/white"

                            />
                    </LinearLayout>













                </LinearLayout>


            </LinearLayout>



        </LinearLayout>






        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Asset Info"
