                    android:src="@drawable/location_pin_icon" />


                <View
                    android:layout_width="0.5dp"
                    android:background="@color/grey"
                    android:layout_marginLeft="4dp"
                    android:layout_height="match_parent"></View>



                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:orientation="horizontal"
                    >

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:orientation="vertical">


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Coordinates"
                            android:textSize="@dimen/bottom_sheet_text_size_items_below"
                            android:layout_marginTop="@dimen/margin_top_bottom_sheet_Items"
                            android:textColor="@color/white"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="GeoFence"
                            android:textSize="@dimen/bottom_sheet_text_size_items_below"
                            android:layout_marginTop="@dimen/margin_top_bottom_sheet_Items"
                            android:textColor="@color/white"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Route Status"
                            android:textSize="@dimen/bottom_sheet_text_size_items_below"
                            android:layout_marginTop="@dimen/margin_top_bottom_sheet_Items"
                            android:textColor="@color/white"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Driver Name"
                            android:textSize="@dimen/bottom_sheet_text_size_items_below"
                            android:layout_marginTop="@dimen/margin_top_bottom_sheet_Items"
                            android:textColor="@color/white"
                            />
                    </LinearLayout>



                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:layout_marginLeft="56dp"
                        android:orientation="vertical">


                        <TextView
                            android:id="@+id/tv_coordinates"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
